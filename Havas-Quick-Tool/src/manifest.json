{
  "name": "Havas Quick Tools",
  "version": "1.0",
  "description": "Automate process",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "./scripts/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "declarativeContent"]
}
